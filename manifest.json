{
    "name": "Super Sema",
    "description" : "With him the game goes like clockwork!",
    "version": "0.1",
    "manifest_version": 2,
    "browser_action": {
      "default_icon": "icon.png"
    },
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["mutation.js"],
          "css": ["styles.css"]
        }
    ],
    "permissions": [
        "activeTab",
        "webRequest",
        "background",
        "tabss"
    ],
    "web_accessible_resources": [
      "i/*"
    ]
}